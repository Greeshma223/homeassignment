<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>ProjectServlet</servlet-name>
        <servlet-class>assign.ProjectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProjectServlet</servlet-name>
        <url-pattern>/projects</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TodoServlet</servlet-name>
        <servlet-class>assign.TodoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TodoServlet</servlet-name>
        <url-pattern>/todos</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>ExportSummaryServlet</servlet-name>
    <servlet-class>assign.ExportSummaryServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ExportSummaryServlet</servlet-name>
    <url-pattern>/export</url-pattern>
</servlet-mapping>
    
</web-app>

